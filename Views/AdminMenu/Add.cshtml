@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


@model WTechCoreSample.Models.VM.AdminMenuVM



<form action="/AdminMenu/Add" method="post">

    <div class="form-group">
        <label>Name</label>
        @Html.TextBoxFor(q => q.Name, new { @class = "form-control" })
    </div>

    <div class="form-group">
        <label>Link</label>
        @Html.TextBoxFor(q => q.Link, new { @class = "form-control" })
    </div>

    <div class="form-group">
        <input class="bt btn-primary" type="submit" value="Add" />
    </div>

</form>