@model List<WTechCoreSample.Models.ORM.WebUser>


@if (Model.Count > 0)
{

    <table>
        <tr>
            <td>Id</td>
            <td>Name</td>
            <td>Surname</td>
            <td>City</td>

        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.SurName</td>
                <td>@item.City</td>
            </tr>
        }
    </table>
}
else
{

   <h1>Bu role ait kullanıcı bulunamamıştır!</h1>


}

