@model List<WTechCoreSample.Models.ORM.Role>


<table>

    <tr>
        <td>Id</td>
        <td>Name</td>
    </tr>


    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td><a href="/Role/WebUsersByRoleId/@item.Id"> @item.Name </a></td>
        </tr>
    }

</table>